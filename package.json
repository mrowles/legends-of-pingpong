{
  "name": "legends-of-pong",
  "version": "1.0.0-alpha.1",
  "repository": "https://github.com/project-vrum/legends-of-pingpong",
  "engine": "node 4.4.5",
  "main": "app.js",
  "scripts": {
    "test": "",
    "css-build": "nodemon -e scss -x \"npm run css-move\"",
    "css-move": "node-sass --include-path scss client/index.scss public/css/styles.css",
    "eslint": "node_modules/eslint/bin/eslint.js server/**/*.js",
    "tslint": "node_modules/tslint/bin/tslint client/**/*.ts  -s node_modules/tslint-stylish -t stylish",
    "js-build": "node_modules/ntypescript/bin/tsc client/**/*.ts",
    "js-move": "node scripts/copy-scripts",
    "start-dev": "concurrently \"npm run css-build\" \"npm run js-build\" \"npm run js-move\" \"node app.js\"",
    "start": "npm run css-move; npm run js-build; npm run js-move; node app.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.10",
    "body-parser": "1.14.2",
    "bootstrap": "3.3.6",
    "cookie-parser": "1.3.5",
    "core-js": "2.4.0",
    "debug": "2.2.0",
    "ejs": "2.4.1",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "express": "4.13.1",
    "method-override": "2.3.5",
    "mongoose": "4.4.5",
    "morgan": "1.6.1",
    "path": "0.12.7",
    "phantomjs-prebuilt": "2.1.7",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "serve-favicon": "2.3.0",
    "systemjs": "0.19.27",
    "underscore": "1.8.3",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "concurrently": "2.0.0",
    "eslint": "^2.11.1",
    "jasmine-core": "2.4.1",
    "karma": "0.13.22",
    "karma-phantomjs-launcher": "1.0.0",
    "node-sass": "3.6.0",
    "nodemon": "1.9.1",
    "ntsc": "^0.2.0",
    "ntypescript": "^1.201605022209.1",
    "recursive-copy": "2.0.3",
    "tslint": "3.8.1",
    "tslint-stylish": "2.1.0-beta"
  }
}
